# Supabase Configuration
# Get these from your Supabase project settings
VITE_SUPABASE_URL=your_supabase_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Service role key for server-side operations (DO NOT EXPOSE TO CLIENT)
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# N8N Integration
# Replace with your actual N8N instance URL and authentication token
VITE_N8N_AUTH_TOKEN=your_n8n_auth_token

# N8N Webhook URLs
# Using your n8n instance base URL
VITE_N8N_APOLLO_WEBHOOK=your_apollo_webhook_url
VITE_N8N_LINKEDIN_WEBHOOK=your_linkedin_webhook_url
VITE_N8N_EMAIL_WEBHOOK=your_email_webhook_url
VITE_N8N_LINKEDIN_OUTREACH_WEBHOOK=your_linkedin_outreach_webhook_url

# Webhook Security
VITE_SUPABASE_WEBHOOK_SECRET=your_webhook_secret

# Development Environment
NODE_ENV=development

# API Keys for third-party integrations
VITE_INSTANTLY_API_KEY=your_instantly_api_key
VITE_HEYREACH_API_KEY=your_heyreach_api_key
# APOLLO_API_KEY=your_apollo_api_key